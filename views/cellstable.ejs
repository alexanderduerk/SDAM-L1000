<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>L1000</title>
  <link rel="stylesheet" href="/css/tablestyles.css" />
  <link rel="stylesheet" href="/css/style.css" />
</head>
<body>
  <script src="/advancedSearch.js"></script>
  <nav class="navbar">
    <div class="container">
      <a href="#" class="logo">L1000</a>
      <ul class="nav-links">
        <li><a href="#">Home</a></li>
        <li><a href="#">About</a></li>
        <li><a href="#">Services</a></li>
        <li><a href="#">Contact</a></li>
      </ul>
      <div class="menu-toggle">
        <span class="bar"></span>
        <span class="bar"></span>
        <span class="bar"></span>
      </div>
    </div>
  </nav>

  <main>
    <% if (data && data.length > 0) { %>
      <table>
        <thead>
          <tr>
            <th>Name</th>
            <th>Cellosaurus ID</th>
            <th>Donor Age</th>
            <th>Doubling Time</th>
            <th>Growthmedium</th>
            <th>Celltype</th>
            <th>Donor ethnicity</th>
            <th>Donor sex</th>
            <th>Donor tumor phase</th>
            <th>Cell lineage</th>
            <th>Primary disease</th>
            <th>Subtype disease</th>
            <th>Provider name</th>
            <th>Growth pattern</th>
          </tr>
        </thead>
        <tbody>
          <% for (const row of data) { %>
            <tr>
              <% for (const value of Object.values(row)) { %>
                <td><%= value %></td>
              <% } %>
            </tr>
          <% } %>
        </tbody>
      </table>
    <% } else { %>
      <p>No data available</p>
    <% } %>
  </main>
</body>
</html>
